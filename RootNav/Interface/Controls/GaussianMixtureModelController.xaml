<UserControl x:Class="RootNav.Interface.Controls.GaussianMixtureModelController"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" Height="59" Width="276">
    <Canvas>
        <CheckBox Foreground="#FFBBBBBB" Canvas.Left="12" Canvas.Top="12" Content="Weighted Components" Height="19" Name="weightedComponentsCheckbox" Width="180" IsChecked="True" Checked="Checkboxes_Checked" Unchecked="Checkboxes_Checked"/>
        <CheckBox Foreground="#FFBBBBBB" Canvas.Left="12" Canvas.Top="35" Content="Show Mixture" Height="19" Name="mixtureDistributionCheckbox" Width="102" Checked="Checkboxes_Checked" Unchecked="Checkboxes_Checked"/>
        <Separator Canvas.Left="0" Canvas.Top="0" Height="6" Name="separator1" Width="262" Margin="6,2" />
        <CheckBox Foreground="#FFBBBBBB" Canvas.Left="161" Canvas.Top="12" Content="Autoscale X Axis" Height="19" IsChecked="False" Name="autoscaleXAxis" Width="107" Checked="Checkboxes_Checked" Unchecked="Checkboxes_Checked"/>
        <CheckBox Foreground="#FFBBBBBB" Canvas.Left="112" Canvas.Top="35" Content="Normalised" Height="19" Name="normalisedCheckbox" Width="144" Checked="Checkboxes_Checked" Unchecked="Checkboxes_Checked"/>
    </Canvas>
</UserControl>
